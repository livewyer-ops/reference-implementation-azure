releases:
  - name: cnoe-argocd-config
    namespace: argocd
    chart: ../charts/raw
    values:
      - argocd.yaml.gotmpl
    disableValidationOnInstall: true
    needs:
      - argocd/argocd
  - name: cnoe-crossplane-addon
    namespace: crossplane-system
    chart: ../charts/raw
    values:
      - crossplane.yaml.gotmpl
    disableValidationOnInstall: true
    needs:
      - crossplane-system/crossplane
